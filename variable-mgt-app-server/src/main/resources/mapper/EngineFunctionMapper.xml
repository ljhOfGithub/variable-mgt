<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.wiseco.var.process.app.server.repository.EngineFunctionMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.wiseco.var.process.app.server.repository.entity.EngineFunction">
        <result column="id" property="id"/>
        <result column="name" property="name"/>
        <result column="label" property="label"/>
        <result column="group_name" property="groupName"/>
        <result column="group_label" property="groupLabel"/>
        <result column="data_type" property="dataType"/>
        <result column="template_need" property="templateNeed"/>
        <result column="template_content" property="templateContent"/>
        <result column="sort_order" property="sortOrder"/>
        <result column="delete_flag" property="deleteFlag"/>
        <result column="created_user" property="createdUser"/>
        <result column="updated_user" property="updatedUser"/>
        <result column="created_time" property="createdTime"/>
        <result column="updated_time" property="updatedTime"/>
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id,
        name, label, group_name, group_label, data_type, template_need, template_content, sort_order, delete_flag,
        created_user, updated_user, created_time, updated_time
    </sql>

</mapper>
